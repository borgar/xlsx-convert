{
  "parser": "@babel/eslint-parser",
  "extends": [ "@borgar/eslint-config" ],
  "env": {
    "es6": true,
    "node": true
  },
  "rules": {
    "newline-per-chained-call": "off",
    "array-element-newline": "off",
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module",
    "requireConfigFile": false
  }
}
