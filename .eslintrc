{
  extends: ['@borgar/eslint-config'],
  "env": { 
    "es6": true,
    "node": true 
  },
  "rules": {
    "newline-per-chained-call": "off"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": 8,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  globals: { Set: true }
}
